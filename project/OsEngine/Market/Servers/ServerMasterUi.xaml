<Window x:Class="OsEngine.Market.Servers.ServerMasterUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Управление серверами доступа" Height="260" Width="341" WindowStartupLocation="CenterScreen" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" Icon="/Images/OsLogo.ico">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <GradientStop Color="#FF323234" Offset="1"/>
            <GradientStop Color="#FFE0E0EA"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Rectangle Margin="5,5,5,15"/>
        <CheckBox x:Name="CheckBoxServerAutoOpen" Content="Разворачивать сервера автоматически" HorizontalAlignment="Left" Margin="19,0,0,30" VerticalAlignment="Bottom"/>
        <WindowsFormsHost Name="HostSource" Margin="10,10,10,52"/>

    </Grid>
</Window>
